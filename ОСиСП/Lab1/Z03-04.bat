@chcp 65001
@echo off
echo -- строка параметров: %*
echo -- параметр 1: %1
echo -- параметр 2: %2
echo -- параметр 3: %3
set /a "a=%1+%2, b=%1*%2, c=%3/%2, d=%2-%1, e=(%2-%1)*(%1-%2)"
echo -- %1 + %2 = %a%
echo -- %1 * %2 = %b%
echo -- %3 / %2 = %c%
echo -- %2 - %1 = %d%
echo -- (%2 - %1)*(%1 - %2) = %e%